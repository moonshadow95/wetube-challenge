extends ../base
include ../mixins/video

block content 
    div
        if !loggedInUser.avatarUrl
            i.fas.fa-user-circle.fa-8x
        else 
            img(src=loggedInUser.avatarUrl alt="Avatar image" crossorigin)
        h3=user.username
    div
        if user.videos
            each video in user.videos
                +video(video)
        else
            span No videos found 